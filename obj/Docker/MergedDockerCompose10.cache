name: dockercompose868788602224343195
services:
  db_mssql:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Mypassw00rd@
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      my_custom_network: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
    volumes:
      - type: bind
        source: C:\mssql\data
        target: /var/opt/mssql
        bind:
          create_host_path: true
  db_mysql:
    cap_add:
      - SYS_NICE
    environment:
      MYSQL_DATABASE: mydb
      MYSQL_ROOT_PASSWORD: passw00rd
    image: mysql:latest
    networks:
      my_custom_network: null
    ports:
      - mode: ingress
        target: 3306
        published: "3346"
        protocol: tcp
    restart: always
    volumes:
      - type: bind
        source: C:\mysql
        target: /var/lib/mysql
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Ararat Grigoryan\Desktop\JWT_API_SECOND_EXPIRIENCE\setup.sql
        target: /docker-entrypoint-initdb.d/setup.sql
        bind:
          create_host_path: true
  jwt_api_second_expirience:
    build:
      context: C:\Users\Ararat Grigoryan\Desktop\JWT_API_SECOND_EXPIRIENCE
      dockerfile: JWT_API_SECOND_EXPIRIENCE/Dockerfile
    depends_on:
      db_mssql:
        condition: service_started
        required: true
      db_mysql:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    links:
      - db_mysql
    networks:
      my_custom_network: null
    ports:
      - mode: ingress
        target: 80
        protocol: tcp
      - mode: ingress
        target: 80
        published: "5000"
        protocol: tcp
      - mode: ingress
        target: 443
        protocol: tcp
      - mode: ingress
        target: 443
        published: "50001"
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\Ararat Grigoryan\AppData\Roaming/Microsoft/UserSecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Ararat Grigoryan\AppData\Roaming/ASP.NET/Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
networks:
  my_custom_network:
    name: dockercompose868788602224343195_my_custom_network
    driver: bridge